[project]
	description = Feeders for DCI Control Server
[access "refs/*"]
  read = group dci-core
  owner = group dci-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group dci-core
	label-Code-Review = -2..+2 group dci-ptl
	label-Verified = -2..+2 group dci-ptl
	label-Workflow = -1..+1 group dci-core
	label-Workflow = -1..+1 group dci-ptl
	label-Workflow = -1..+0 group Registered Users
	submit = group dci-ptl
	read = group dci-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group dci-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
